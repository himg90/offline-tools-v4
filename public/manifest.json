{
  "manifest_version": 3,
  "name": "Universal Decoder Extension",
  "version": "1.0",
  "description": "Utility for decoding various encoded strings",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}